---
import RootAuthenticatedLayout from "../layouts/RootAuthenticatedLayout.astro";
import { FlashcardGenerationView } from "../components/generate/FlashcardGenerationView";

export const prerender = false;

const user = Astro.locals.user;

if (!user) {
  return Astro.redirect("/login");
}
---

<RootAuthenticatedLayout title="Generate Flashcards">
  <FlashcardGenerationView client:load />
</RootAuthenticatedLayout>
